{
  "name": "tree-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "start-electron": "electron .",
    "dev:electron": "concurrently \"next dev\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.pancake.tree-world",
    "files": [
      "out/**/*",
      "main.js"
    ],
    "directories": {
      "output": "dist"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "ag-grid-community": "^33.3.1",
    "ag-grid-react": "^33.3.1",
    "antd": "^5.25.3",
    "axios": "^1.9.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rsuite": "^5.82.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "electron": "^36.3.2",
    "electron-builder": "^26.0.12",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^8.0.3"
  }
}
